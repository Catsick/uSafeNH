<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.usafenh.MainActivity"
    android:background="@android:color/white">






    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:id="@+id/UNH_button"
        android:src="@mipmap/unh_icon"
        android:background="@android:color/white"
        android:nestedScrollingEnabled="false"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="114dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Saint_Anselm"
        android:src="@mipmap/sac"
        android:background="@android:color/white"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/UNH_button" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Keene_State"
        android:src="@mipmap/keene"
        android:background="@android:color/white"
        android:layout_alignTop="@+id/UNH_button"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Plymouth_State"
        android:src="@mipmap/plymouth"
        android:background="@android:color/white"
        android:layout_alignTop="@+id/White_Mountain"
        android:layout_toRightOf="@+id/Saint_Anselm"
        android:layout_toEndOf="@+id/Saint_Anselm" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/White_Mountain"
        android:src="@mipmap/wmcc"
        android:background="@android:color/white"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/Keene_State"
        android:layout_toStartOf="@+id/Keene_State" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What college are you associated with?"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="@android:color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textAlignment="center" />

</RelativeLayout>

